[External Scripts][Paste to Hastebin]
command=a=$(cat); curl -X POST -s -d "$a" https://hastebin.com/documents | awk -F '"' '{print "https://hastebin.com/"$4}' | xargs xdg-open ;
errorMode=0
filterMode=0
inputMode=2
name=Paste to Hastebin
outputMode=0
saveMode=0
shortcuts=
showOutput=false

